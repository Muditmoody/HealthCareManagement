@model HealthCareManagement.Models.Patient[]

@{
    ViewData["Title"] = "View";
}

<h1>View</h1>

<div>
    <h4>Patient</h4>
    <hr />
    @foreach(var m in Model)
    {
    <dl class="row">
        <dt class = "col-sm-2">
            @nameof(m.PatientId)
        </dt>
        <dd class = "col-sm-10">
            @( m.PatientId)
        </dd>
        <dt class = "col-sm-2">
            @nameof(m.PatientName)
        </dt>
        <dd class = "col-sm-10">
            @( m.PatientName)
        </dd>
        <dt class = "col-sm-2">
            @nameof(m.DateOfBirth)
        </dt>
        <dd class = "col-sm-10">
            @( m.DateOfBirth)
        </dd>
    </dl>
    }
</div>
<div>
